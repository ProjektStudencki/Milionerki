<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <color name="title">#ffccf00c</color>
    <color name="fff">#ffffffff</color>
    <color name="text">#ff8aacd8</color>
    <color name="black">#ff000000</color>
    <color name="blue">#ff33acdb</color>
    <color name="bg">#ff232d48</color>

    <color name="ranking1">#ffd700</color>
    <color name="ranking2">#b8b8b8</color>
    <color name="ranking3">#b26434</color>
    <color name="rankin_num">#ffa200</color>

    <color name="txt_gold">#e2cd1f</color>

    <color name="btn">#ff66c9e2</color>
    <color name="btn_active">#ff4fe243</color>
    <color name="btn_choose">#b8b8b8</color>
    
</resources>
